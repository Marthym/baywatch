type User {
    _id: ID
    _createdAt: String
    login: String
    name: String
    mail: String
    roles: [String]
}

type Session {
    user: User
    maxAge: Int
}

input ChangePasswordForm {
    oldPassword: String
    newPassword: String
}

input UserForm {
    login: String
    name: String
    mail: String
    password: String
    roles: [String]
}

type SearchUsersResponse {
    entities: [User]
    totalCount: Int
}
