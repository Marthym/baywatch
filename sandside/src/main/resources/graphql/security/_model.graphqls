type User {
    _id: ID
    _createdAt: String
    _createdBy: String
    _loginAt: String
    _loginIP: String
    login: String
    name: String
    mail: String
    roles: [String]
}

type UserSettings {
    _id: ID
    preferredLocale: String
}

input UserSettingsForm {
    preferredLocale: String
}

type Session {
    user: User
    maxAge: Int
}

input UserForm {
    login: String
    name: String
    mail: String
    password: String
    roles: [String]
}

type SearchUsersResponse {
    entities: [User]
    totalCount: Int
}
