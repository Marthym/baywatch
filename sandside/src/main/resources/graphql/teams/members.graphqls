type TeamMember {
    _id: ID
    _createdAt: String
    _createdBy: User
    _user: User
    userId: String!
    pending: String
}

extend type Query {
    teamMembersList(teamId: ID): [TeamMember]
}

extend type Mutation {
    teamMembersAdd(_id: ID, membersIds: [ID]): [TeamMember]
    teamMembersDelete(_id: ID, membersIds: [ID]): [TeamMember]
}