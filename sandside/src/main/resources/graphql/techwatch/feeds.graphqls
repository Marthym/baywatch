type Feed {
    _id: ID!,
    _createdBy: String,
    name: String,
    description: String,
    location: String!,
    tags: [String],
    error: ScrapingError,
}

input FeedForm {
    name: String
    location: String
    description: String
    tags: [String]
}

type SearchFeedsResponse {
    entities: [Feed]
    totalCount: Int
}

extend type Query {
    getFeed(id: ID): Feed
    feedsSearch(
        _p: Int
        _pp: Int
        _from: Int
        _to: Int
        _s: String
        id: [ID]
        name: String
        url: String
        lastWatch: String
        tags: [String]
    ): SearchFeedsResponse
}

extend type Mutation {
    feedAddAndSubscribe(feed: FeedForm): Feed
    feedUpdate(id: ID, name: String, description: String, tags: [String]): Feed
    subscribe(id: ID, name: String, tags: [String]): Feed
}